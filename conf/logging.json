{
  "version":1,
  "disable_existing_loggers":false,
  "formatters":{
    "main":{
      "format":"[%(asctime)s] %(levelname)-7s [%(funcName)s: %(filename)s]%(lineno)d -- %(message)s"
    },
    "access":{
      "format":"[%(asctime)s] %(message)s"
    }
  },
  "handlers":{
    "console":{
      "class":"logging.StreamHandler",
      "formatter":"main"
    },
    "autop-rotate-file":{
      "filename":"/home/jianqiao/workspace/autop/logs/autop.log",
      "class":"logging.handlers.TimedRotatingFileHandler",
      "formatter":"main",
      "when":"D",
      "backupCount":30
    },
    "access-console":{
      "class":"logging.StreamHandler",
      "formatter":"access"
    },
    "access-file":{
      "filename":"/home/jianqiao/workspace/autop/logs/access.log",
      "class":"logging.handlers.TimedRotatingFileHandler",
      "formatter":"access",
      "when":"D",
      "backupCount":30
    },
    "tornado-file":{
      "filename":"/home/jianqiao/workspace/autop/logs/tornado.log",
      "class":"logging.handlers.TimedRotatingFileHandler",
      "formatter":"main",
      "when":"D",
      "backupCount":30
    }
  },
  "loggers":{
    "":{
      "level":"DEBUG"
    },
    "autop":{
      "handlers":[
        "console",
        "autop-rotate-file"
      ]
    },
    "tornado.access":{
      "handlers":[
        "access-console",
        "access-file"
      ]
    },
    "tornado.application":{
      "handlers":[
        "console",
        "tornado-file"
      ]
    },
    "tornado.general":{
      "handlers":[
        "console",
        "tornado-file"
      ]
    }
  }
}